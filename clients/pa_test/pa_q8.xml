<etldataflow name="Q921.8_1">
<globals>
	  <param name="ETLFLOW_DESCR" value="Line #9 on Q-921 list."></param>

	<!-- PIPELINE execution type
		 SYNC - synchronous, ANYNC - asynchronous 
	-->
	  <param name="FLOW_TYPE" value="SYNC"></param>	  

	  <param name="FIELD_TERMINATOR" value="'|'"></param>
	  <param name="LINE_TERMINATOR" value="''"></param>
	  
	  <param name="DB_CONNECTOR" value="%PADB_MAP%"></param>
	  <param name="SCHEMA_NAME" value="PARACCEL"></param>
	   <!--will spool ALL records if LAME_DUCK=0-->
	  <param name="LAME_DUCK" value='0'></param>
	  <param name="ARRAYSIZE" value="5000"></param>
	  <param name="COPYCOMMIT" value="4"></param>
	  <!--sqlp attribute-->


	  <param name="TABLE_NAME" value="TRD_VOL_UNION"></param>

	   

</globals>

<worker name="Q8_TVU_CLEAR_CACHE">
  <exec_pa_dml>
    <psql 	method="%PA_DML_TEST%" >
	<param name="DESCR" value="Clear cache."></param>
      <sql_template>
       <![CDATA[
	   xpx 'toss_cache';
		]]>
      </sql_template>
    </psql>
  </exec_pa_dml>
</worker>
	  

<worker name="Q8_0">
  <exec_pa_dml>
    <psql 	method="%PA_DML_TEST%" >
	<param name="DESCR" value="Query from Q-921 list."></param>
	<param name="TVU_LIMIT" value="100000"></param>
	<param name="TVU" value="TRD_VOL_UNION"></param>
      <sql_template>
        <![CDATA[SELECT ASET_LVL_KEY, count(trd_vol_sid) as a  FROM TRD_VOL_UNION Where src_file_Cd in ('DOGS','IOWA')  and COB_DT = '2011-05-19' group by ASET_LVL_KEY order by a;
		]]>
      </sql_template>
    </psql>
  </exec_pa_dml>
</worker>

</etldataflow>