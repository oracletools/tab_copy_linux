        <param name="PARTITION" value="OCT_12"></param>
 <param name="NUM_OF_SHARDS" value="20"></param>
     <param name="IF_DPL_SERIAL" value="0"></param>

      <sql_template>
        <![CDATA[CSMARTVOL.STG_TRD_VOL_OTC_DERV_1]]>
      </sql_template>
    </sqlp>
  </exec_copy>
</worker>
</etldataflow>
