	  <param name="FIELD_TERMINATOR" value="|^"></param>
	  <param name="LINE_TERMINATOR" value="~"></param>
	  <param name="FROM_DB" value="%AB_SMARTP1B%"></param>	  
	  <param name="TO_DB" value="%CVOL_SMARTQ1%"></param>
