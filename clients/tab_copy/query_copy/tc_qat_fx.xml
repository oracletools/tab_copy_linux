<etldataflow name="Q_COPY2_TEST">
<globals> 
 
	<!-- PIPELINE execution type
		 SYNC - synchronous, ANYNC - asynchronous 
	-->
	  <param name="FLOW_TYPE" value="ASYNC"></param>	

	  <param name="FIELD_TERMINATOR" value="`^|"></param>
	  <param name="LINE_TERMINATOR" value="~"></param>
	  
  
	  <param name="FROM_DB" value="%AD_SMARTP2%"></param>
	  <param name="FROM_SCHEMA" value="CSMARTOTD"></param>
	  
	  <param name="TO_DB" value="%COTD_SMARTU2%"></param>
	  <param name="TO_SCHEMA" value="CSMARTOTD"></param>	  
	  
	   <!--will spool ALL records if LAME_DUCK=0-->
	  <param name="LAME_DUCK" value='0'></param>
	  <param name="ARRAYSIZE" value="5000"></param>
	  <param name="COPYCOMMIT" value="4"></param>
	  <!--sqlp attribute-->

	  <param name="IF_TRUNCATE" value="0"></param>
	  
	  <param name="IF_SHOW_SERVEROUTPUT" value="1"></param>
</globals>


<worker name="Q1">
  <exec_query_copy>
    <table_utils        method="%QUERY_COPY_METHOD%" >
         <param name="TO_TABLE" value="OTD_VOL_FX"></param>
	<param name="IF_CREATE_TARGET_TABLE" value="0"></param>
      <sql_template>
        <![CDATA[select * from CSMARTOTD.OTD_VOL_FX WHERE COB_DT ='18-JUN-2015' and src_file_cd  in ('FLXFXAU','FLXFXBD','FLXFXCGMA','FLXFXCN','FLXFXHK','FLXFXID','FLXFXIN','FLXFXKR','FLXFXLK','FLXFXMY','FLXFXPH','FLXFXSG','FLXFXTH','FLXFXTW','FLXFXVN','FXJPCJL','FXMLVCD1','FXMLVCI1','FXMLVCM1','FXMLVGA1','FXMLVKE1','FXMLVNG1','FXMLVSN1','FXMLVTZ1','FXMLVUG1','FXMLVZA1','FXMLVZM1')]]>

      </sql_template>
   </table_utils>
  </exec_query_copy>

</worker>



</etldataflow>
