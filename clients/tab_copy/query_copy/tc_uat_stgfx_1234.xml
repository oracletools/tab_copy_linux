<etldataflow name="Q_COPY2_TEST">
<globals> 
 
	<!-- PIPELINE execution type
		 SYNC - synchronous, ANYNC - asynchronous 
	-->
	  <param name="FLOW_TYPE" value="ASYNC"></param>	

	  <param name="FIELD_TERMINATOR" value="`|"></param>
	  <param name="LINE_TERMINATOR" value="~"></param>
	  
  
	  <param name="FROM_DB" value="%BK_SMARTC1B%"></param>
	  <param name="FROM_SCHEMA" value="CSMARTVOL"></param>
	  
	  <param name="TO_DB" value="%CVOL_SMARTU1B%"></param>
	  <param name="TO_SCHEMA" value="CSMARTVOL"></param>	  
	  
	   <!--will spool ALL records if LAME_DUCK=0-->
	  <param name="LAME_DUCK" value='0'></param>
	  <param name="ARRAYSIZE" value="5000"></param>
	  <param name="COPYCOMMIT" value="4"></param>
	  <!--sqlp attribute-->

	  <param name="IF_TRUNCATE" value="1"></param>
	  
	  <param name="IF_SHOW_SERVEROUTPUT" value="1"></param>
</globals>



<worker name="Q3">
  <exec_query_copy>
    <table_utils        method="%QUERY_COPY_METHOD%" >
        <param name="TO_TABLE" value="TRD_VOL_UNION_12"></param>
        <param name="IF_CREATE_TARGET_TABLE" value="1"></param>
      <sql_template>
        <![CDATA[select /*+PARLLEL(FX1,8)*/* from CSMARTVOL.trd_vol_union  FX1 WHERE cob_dt between '01-JAN-2011' and '31-DEC-2014'  ]]>
      </sql_template>
   </table_utils>
  </exec_query_copy>

</worker>

<worker name="Q3">
  <exec_query_copy>
    <table_utils        method="%QUERY_COPY_METHOD%" >
        <param name="TO_TABLE" value="TRD_VOL_UNION_12_1"></param>
        <param name="IF_CREATE_TARGET_TABLE" value="1"></param>
      <sql_template>
        <![CDATA[select /*+PARLLEL(FX1,8)*/* from CSMARTVOL.TRD_VOL_UNION_1  FX1 WHERE   cob_dt between '01-jan-2011' and '31-dec-2014']]>
      </sql_template>
   </table_utils>
  </exec_query_copy>

</worker>



</etldataflow>
