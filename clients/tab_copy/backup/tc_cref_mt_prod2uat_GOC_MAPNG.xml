    <sqlp       method="%COPY_METHOD%" >
      <sql_template>
        <![CDATA[
CSMARTBSER.MAN_REF_GOC_MAPNG
]]>
      </sql_template>
    </sqlp>
  </exec_dml>
</worker>

</etldataflow>
