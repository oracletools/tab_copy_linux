<etldataflow name="TEST_COPY">
<globals>
	<!-- PIPELINE execution type
		 SYNC - synchronous, ANYNC - asynchronous 
	-->
	  <param name="FLOW_TYPE" value="ASYNC"></param>	  

	  <param name="FIELD_TERMINATOR" value='|'></param>
	  <param name="LINE_TERMINATOR" value="~"></param>
	  <param name="FROM_DB" value="%CVOL_SMARTD1%"></param>
	  <param name="TO_DB" value="%ESMARTVOL_SMARTQ1%"></param>
	  <param name="TO_SCHEMA" value="ESMARTVOL"></param>
	  <param name="LAME_DUCK" value='0'></param>
	  <param name="ARRAYSIZE" value="5000"></param>
	  <param name="COPYCOMMIT" value="4"></param>
	  <!--Truncate table/partition before inserting -->
	  <param name="IF_TRUNCATE" value="1"></param>	  
	  <!--sqlp attribute-->

</globals>



<worker1 name="CSMARTVOL.ZDP_FLEX_MM_OPEN_TRD">
  <exec_dml>
    <sqlp 	method="%COPY_METHOD%" >
	<param name="FILTER1" value=" COB_DT>SYSDATE-2 "></param>
      <sql_template>
        <![CDATA[
CSMARTVOL.ZDP_FLEX_MM_OPEN_TRD
]]>

      </sql_template>
    </sqlp>
  </exec_dml>
</worker1>

<worker name="CSMARTVOL.ZDP_FLEX_FX_OPEN_TRD">
  <exec_dml>
    <sqlp       method="%COPY_METHOD%" >
        <param name="FILTER1" value=" COB_DT>SYSDATE-2 "></param>
      <sql_template>
        <![CDATA[
CSMARTVOL.ZDP_FLEX_FX_OPEN_TRD
]]>

      </sql_template>
    </sqlp>
  </exec_dml>
</worker>



<worker1 name="REF_CTP_GRAND_PRNT_LIST">
  <exec_select>
    <sqlp 	method="%SELECT_METHOD%" >
	<param name="DB_CONNECTOR" value="%ESMARTREF_SMARTD1%"></param>
	<param name="SCHEMA_NAME" value="ESMARTREF"></param>	
      <sql_template>
        <![CDATA[
SELECT COUNT(*)||'|cnt' FROM ESMARTREF.REF_CTP_GRAND_PRNT_LIST;
]]>
      </sql_template>
    </sqlp>
  </exec_select>
</worker1>

<worker1 name="REF_ESALES">
  <exec_dml>
    <sqlp 	method="%COPY_METHOD%" 
			method1="sql_plus_copy">
	  <param name="FIELD_TERMINATOR" value='||'></param>
	  <param name="LINE_TERMINATOR" value="~"></param>
      <sql_template>
        <![CDATA[
CSMARTREF.REF_ESALES
]]>
      </sql_template>
    </sqlp>
  </exec_dml>
</worker1>


</etldataflow>
