<etldataflow name="CSMARTBSER.GMRS_HISTORY_P2U">
<globals>

	<!-- PIPELINE execution type
		 SYNC - synchronous, ANYNC - asynchronous 
	-->
	  <param name="FLOW_TYPE" value="ASYNC"></param>	 
	  
	  <param name="FIELD_TERMINATOR" value='|'></param>
	  <param name="LINE_TERMINATOR" value="~"></param>

	  <param name="TO_SCHEMA1" value="CSMARTVOL"></param>
	  <param name="LAME_DUCK" value='0'></param>
	  <param name="ARRAYSIZE" value="5000"></param>
	  <param name="COPYCOMMIT" value="4"></param>
	  <!--Truncate table/partition before inserting -->
	  <param name="IF_TRUNCATE" value="0"></param>
	  <!--sqlp attribute-->
	  <param name="TO_TABLE" value="NOSTROS_HISTORY"></param>
	  

</globals>




<worker name="NS_DEL">
  <exec_select>
    <sqlp   method="%SELECT_METHOD%" >
        <!--DATE_RANGE_DELTA - there's a RANGE partiton on DATE field -->
		<param name="FIELD_TERMINATOR" value='|'></param>
		<param name="DB_CONNECTOR" value="%CSMARTBSER_SMARTU1B%"></param>
		<param name="SCHEMA_NAME" value="CSMARTBSER"></param>
      <sql_template>
        <![CDATA[
DELETE csmartbser.nostros_summary
where
cob_date in ('15-AUG-2011', '16-AUG-2011', '17-AUG-2011', '18-AUG-2011', '22-AUG-2011', '23-AUG-2011', 24-AUG-2011', '25-AUG-2011', '29-AUG-2011', '30-AUG-2011', '31-AUG-2011')
and
sourcesystem='TLM-LATAM'
and
src_file_cd='UM';
COMMIT;
]]>
      </sql_template>
    </sqlp>
  </exec_select>
</worker>

<worker1 name="NH_COPY">
  <exec_copy>
    <sqlp   method="%COPY_METHOD%" >
        <!--DATE_RANGE_DELTA - there's a RANGE partiton on DATE field -->
		<param name="FIELD_TERMINATOR" value='|'></param>
		<param name="TO_DB" value="%CSMARTBSER_SMARTU1B%"></param>
		<param name="FILTER" value="cob_date in ('15-AUG-2011', '16-AUG-2011', '17-AUG-2011', '18-AUG-2011', '22-AUG-2011', '23-AUG-2011', '24-AUG-2011', '25-AUG-2011', '29-AUG-2011', '30-AUG-2011', '31-AUG-2011') and sourcesystem='TLM-LATAM' and src_file_cd='UM'"></param>
      <sql_template>
        <![CDATA[
CSMARTBSER.NOSTROS_HISTORY
]]>
      </sql_template>
    </sqlp>
  </exec_copy>
</worker1>



</etldataflow>